<p id="notice"><%= notice %></p>

<h1>Game States</h1>

<table>
  <thead>
    <tr>
      <th>Whitepieces</th>
      <th>Blackpieces</th>
      <th>Board</th>
      <th>Turn</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @game_states.each do |game_state| %>
      <tr>
        <td><%= game_state.whitePieces %></td>
        <td><%= game_state.blackPieces %></td>
        <td><%= game_state.board %></td>
        <td><%= game_state.turn %></td>
        <td><%= link_to 'Show', game_state %></td>
        <td><%= link_to 'Edit', edit_game_state_path(game_state) %></td>
        <td><%= link_to 'Destroy', game_state, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Game State', new_game_state_path %>
